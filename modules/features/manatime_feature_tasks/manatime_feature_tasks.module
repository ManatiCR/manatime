include_once 'manatime_feature_tasks.features.inc';

/**
 * Add path aliases.
 */
function tasks_content_types_create_path_alias($source, $alias, $language) {
  $path = array(
    'source' => $source,
    'alias' => $alias,
    'language' => $language,
  );
  path_save($path);
}
